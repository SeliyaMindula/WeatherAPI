{
    "version": 2,
    "name":"WeatherAPI",
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "crons": [
      {
        "path": "/cron/cronjob",
        "schedule": "* * * * *"
      }
    ]
  }
 